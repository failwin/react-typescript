[include]

[ignore]
.*/_old/.*
.*/node_modules/.*
.*/dist/.*
.*/styleguide/.*

[libs]
flow-typed/

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
